{
    "dataRoles": [
        {
            "displayName": "Data Columns",
            "name": "dataColumns",
            "kind": "Grouping"
        },
        {
            "displayName": "Event Data",
            "name": "eventData",
            "kind": "Grouping"
        },
        {
            "displayName": "Similar Data IDs",
            "name": "similarDataIds",
            "kind": "Grouping"
        },
        {
            "displayName": "Similar Data Duration",
            "name": "similarDataDuration",
            "kind": "Grouping"
        },
        {
            "displayName": "ID",
            "name": "dataID",
            "kind": "Grouping"
        }
    ],
    "objects": {
        "general": {
            "displayName": "General",
            "displayNameKey": "formattingGeneral",
            "properties": {
                "filter": {
                    "type": {
                        "filter": true
                    }
                },
                "selfFilter": {
                    "type": {
                        "filter": {
                            "selfFilter": false
                        }
                    }
                }
            }
        },
        "dataReductionCustomization": {
            "displayName": "Data Reduction",
            "properties": {
                "rowCount": {
                    "type": {
                        "numeric": true
                    },
                    "displayName": "Row Reduction",
                    "description": "Show Reduction for all row groups",
                    "suppressFormatPainterCopy": true
                }
            }
        },
        "filterSettings": {
            "displayName": "Filter Settings",
            "properties": {
                "filterSimilarDefault": {
                    "type": {
                        "bool": true
                    }
                },
                "similarIDTable": {
                    "type": {
                        "text": true
                    }
                },
                "similarIDColumn": {
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "lineupSettings": {
            "displayName": "Lineup",
            "displayNameKey": "Lineup",
            "properties": {
                "jsondump": {
                    "type": {
                        "text": true
                    }
                },
                "showSidePanel": {
                    "type": {
                        "bool": true
                    }
                },
                "overviewMode": {
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "lineUpVisualSettings": {
            "displayName": "Lineup Settings",
            "properties": {
                "aggregationStrategy": {
                    "type": {
                        "text": true
                    }
                },
                "customMapping": {
                    "type": {
                        "text": true
                    }
                }
            }
        },
        "eventSettings": {
            "displayName": "Event Settings",
            "properties": {
                "eventUnit": {
                    "type": {
                        "enumeration": []
                    }
                },
                "msPerEventUnit": {
                    "type": {
                        "numeric": true
                    }
                },
                "boxplotUnit": {
                    "type": {
                        "enumeration": []
                    }
                },
                "msPerBoxplotUnit": {
                    "type": {
                        "numeric": true
                    }
                },
                "eventScaleMin": {
                    "type": {
                        "numeric": true
                    }
                },
                "eventScaleMax": {
                    "type": {
                        "numeric": true
                    }
                },
                "heatmapBinCount": {
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "zoomingSettings": {
            "displayName": "Zooming Settings",
            "properties": {
                "zoomingEnabled": {
                    "type": {
                        "bool": true
                    }
                },

                "minZoomingScale": {
                    "type": {
                        "numeric": true
                    }
                },
                "maxZoomingScale": {
                    "type": {
                        "numeric": true
                    }
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "eventData": { "min": 1 },
                    "similarDataIds": { "max": 1 },
                    "similarDataDuration": { "max": 1 },
                    "dataID": { "max": 1 }
                },
                {
                    "similarDataIds": { "max": 1 },
                    "similarDataDuration": { "max": 1 },
                    "dataID": { "max": 1 }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "dataColumns"
                            }
                        },
                        {
                            "for": {
                                "in": "eventData"
                            }
                        },
                        {
                            "for": {
                                "in": "similarDataIds"
                            }
                        },
                        {
                            "for": {
                                "in": "similarDataDuration"
                            }
                        },
                        {
                            "for": {
                                "in": "dataID"
                            }
                        }
                    ],

                    "dataReductionAlgorithm": {
                        "window": { "count": 20001 }
                    }
                }
            }
        }
    ],
    "dataReductionCustomization": {
        "categorical": {
            "categoryCount": {
                "propertyIdentifier": {
                    "objectName": "dataReductionCustomization",
                    "propertyName": "rowCount"
                },
                "defaultValue": 100
            }
        }
    },
    "privileges": [],
    "supportsLandingPage": true,
    "supportsSynchronizingFilterState": true,
    "tooltips": {
        "supportedTypes": {
            "default": true,
            "canvas": true
        },
        "roles": ["tooltips"]
    }
}
